<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<TitledPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <content>
        <VBox prefHeight="772.0" prefWidth="998.0">
            <children>
                <VBox alignment="CENTER" prefHeight="500.0" prefWidth="506.0">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="244.0" text="Dates(YYYY-MM-DD, YYYY-MM-DD,)" />
                        <TextArea fx:id="dataEntrada" prefHeight="219.0" prefWidth="500.0" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <HBox alignment="CENTER" prefHeight="91.0" prefWidth="977.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="27.0" prefWidth="158.0" text="Client" />
                        <TextField fx:id="client" />
                        <Button mnemonicParsing="false" onAction="#setReserva" text="Reservar">
                            <HBox.margin>
                                <Insets left="20.0" />
                            </HBox.margin>
                        </Button>
                        <Text fx:id="Res" strokeType="OUTSIDE" strokeWidth="0.0">
                            <HBox.margin>
                                <Insets left="40.0" />
                            </HBox.margin>
                        </Text>
                    </children>
                </HBox>
            </children>
        </VBox>
    </content>
</TitledPane>
